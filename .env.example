# Copy to .env and fill in your token
TELEGRAM_BOT_TOKEN=
LOG_LEVEL=INFO
# Rate limit: max generations per user per minute
RATE_LIMIT_PER_MIN=3

# EVM RPC endpoints for /bal bulk balance checks
# Provide your own provider URLs (Infura/Alchemy/Ankr/etc.)
RPC_ETH=
RPC_BASE=
RPC_BSC=
RPC_POLYGON=
RPC_AVAXC=

# Optional: admin user IDs (comma-separated Telegram numeric IDs)
ADMIN_USER_IDS=

# Optional: data directory for local DB and jobs metadata
# profiles.db and jobs.json will live here (default is repository root)
DATA_DIR=.

# Limits for simple bulk generation (/bulk) and massive bulk ZIP (/bulkzip)
# /bulk (in-memory CSV)
BULK_MAX_COUNT=1000

# /bulkzip (chunked CSVs written to disk and zipped)
BULKZIP_CSV_CHUNK=10000
BULKZIP_ZIP_CSVS=10
BULKZIP_MAX_NONADMIN=100000
BULKZIP_MAX_COUNT=1000000

# Optional nonâ€‘EVM balance checks (disabled by default)
# BTC via Blockstream-compatible API
ENABLE_BTC_BAL=0
BTC_API_BASE=https://blockstream.info/api

# Solana via JSON-RPC
ENABLE_SOL_BAL=0
SOL_RPC_URL=

# TON via toncenter-compatible API
ENABLE_TON_BAL=0
TON_API_BASE=
TON_API_KEY=
